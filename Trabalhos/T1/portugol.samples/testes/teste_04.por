algoritmo tc04;
variaveis
    inteiro: i,j,k;
inicio
    i<-1;j<-1;k<-1;
    enquanto(I<11) faca
        enquanto (j<12) faca
           enquanto (k<13) faca
              k<-k+3;
           fim enquanto;
           j<-j+2 //Falta ponto-e-virgula
           i<-4;
        fim enquanto;
        i<-i+1;
    fim enquanto;
fim.
